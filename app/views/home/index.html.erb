<%= render 'shared/form.html.erb' %>

<h1>Home#index</h1>
<p>登録順</p>

<ul id="words">
<% @words.each do |word| %>
  <li class="word">
    <div class="english">
      <%= word.english %>
    </div>
    <div class="japanese" style="display:none;">
      <%= word.japanese %>
    </div>
  </li>
<% end %>
</ul>
